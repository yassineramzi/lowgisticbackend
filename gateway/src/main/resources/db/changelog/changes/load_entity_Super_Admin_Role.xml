<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- Load data on roles -->
    <changeSet id="2022_05_06_13_44" author="">
        <loadData
                file="db/changelog/data/role_super_admin.csv"
                separator=";"
                tableName="roles">
            <column name="id" type="numeric"/>
            <column name="nom_role" type="string"/>
            <column name="description" type="string"/>
        </loadData>
    </changeSet>

    <!-- Load data on utilisateur -->
    <changeSet id="2022_05_06_13_48" author="">
        <loadData
                file="db/changelog/data/utlisateur_super_adminisrateur.csv"
                separator=";"
                tableName="utilisateur">
            <column name="id" type="numeric"/>
            <column name="nom" type="string"/>
            <column name="prenom" type="string"/>
            <column name="login" type="string"/>
            <column name="password" type="string"/>
            <column name="email" type="string"/>
            <column name="fixe" type="string"/>
            <column name="mobile" type="string"/>
        </loadData>
    </changeSet>

    <!-- Load data on utilisateur_roles -->
    <changeSet id="2022_05_06_13_49" author="">
        <loadData
                file="db/changelog/data/utilisateur_role_super_admin.csv"
                separator=";"
                tableName="utilisateur_roles">
            <column name="utilisateur_id" type="numeric"/>
            <column name="roles_id" type="numeric"/>
        </loadData>
    </changeSet>

</databaseChangeLog>